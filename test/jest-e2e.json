{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleDirectories": ["<rootDir>/../", "node_modules"],
 "moduleNameMapper": {
    "^\\.\\/internal\\/class\\.js$": "<rootDir>/../generated/prisma/internal/class.js",
    "^\\.\\/enums\\.js$": "<rootDir>/../generated/prisma/enums.js",
    "^\\.\\/internal\\/prismaNamespace\\.js$": "<rootDir>/../generated/prisma/internal/prismaNamespace.js"

  }
}